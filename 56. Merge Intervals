class Solution {
    public int[][] merge(int[][] intervals) {
        Arrays.sort(intervals,(a,b)->a[0]!=b[0]?Integer.compare(a[0],b[0]):Integer.compare(a[1],b[1]));
        ArrayList<ArrayList<Integer>> ans=new ArrayList<>();
        for(int i=0;i<intervals.length;i++){
            if(ans.size()==0||intervals[i][0]>ans.get(ans.size()-1).get(1)){
                ans.add(new ArrayList<>());
                ans.get(ans.size()-1).add(intervals[i][0]);
                ans.get(ans.size()-1).add(intervals[i][1]);
            }
            else
                ans.get(ans.size()-1).set(1,Math.max(intervals[i][1],ans.get(ans.size()-1).get(1)));
        }
        int[][] res=new int[ans.size()][2];
        for(int i=0;i<ans.size();i++){
            res[i][0]=ans.get(i).get(0);
            res[i][1]=ans.get(i).get(1);
        }
        return res;
    }
}
